{
  "title": "Scouting Entry Upload Schema",
  "description": "One or more scouting entries uploaded by a scouter",
  "type": "object",
  "properties": {
    "title": {
      "description": "The table to insert the request into",
      "type": "string",
      "enum": [
        "pits",
        "matches"
      ]
    }
  },
  "allOf": [
    {
      "if": {
        "properties": {
          "title": {
            "type": "string",
            "const": "pits"
          }
        },
        "required": [
          "title"
        ]
      },
      "then": {
        "properties": {
          "header": {
            "description": "The headers of the columns of the scouting entries",
            "type": "array",
            "const": [
              "scouter_name",
              "team_number",
              "length",
              "width",
              "start_height",
              "max_height",
              "weight",
              "dt_type",
              "drive_motor",
              "swerve_type",
              "wheel_rep_freq",
              "bumper_height",
              "can_bus_top",
              "pneumatics",
              "programming_language",
              "num_batt",
              "does_batt_maint",
              "batt_maint_txt",
              "batt_testing",
              "batt_brand",
              "batt_needed",
              "spare_parts",
              "pit_checklist",
              "does_scouting",
              "trade_scouting_data",
              "batt_storchrg",
              "batt_loc",
              "batt_con",
              "powersw_loc",
              "radio_loc",
              "misc_pics",
              "comments",
              "ScoutedTime"
            ]
          },
          "values": {
            "description": "The list of entries to be added into the database",
            "type": "array",
            "minItems": 1,
            "uniqueItems": true,
            "items": {
              "type": "array",
              "minItems": 33,
              "maxItems": 33,
              "items": {
                "type": "string"
              }
            }
          }
        },
        "required": [
          "header",
          "values"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "title": {
            "type": "string",
            "const": "matches"
          }
        },
        "required": [
          "title"
        ]
      },
      "then": {
        "properties": {
          "header": {
            "description": "The headers of the columns of the scouting entries",
            "type": "array",
            "const": [
              "event_key",
              "match_level",
              "match_number",
              "team_data",
              "scouter_name",
              "starting_pos",
              "mobility",
              "auto_amp",
              "zone1_shot_made_auto",
              "zone1_shot_miss_auto",
              "zone2_shot_made_auto",
              "zone2_shot_miss_auto",
              "under_stage",
              "defense",
              "pickup_method",
              "zone1_shot_made",
              "zone1_shot_miss",
              "zone2_shot_made",
              "zone2_shot_miss",
              "zone3_shot_made",
              "zone3_shot_miss",
              "zone4_shot_made",
              "zone4_shot_miss",
              "teleop_amp",
              "piece_stolen",
              "wait_time",
              "climb_fail",
              "rob_onstage",
              "climb_order",
              "trap_note_pos",
              "spot_try",
              "spot_made",
              "rsl_solid",
              "rsl_off",
              "brown_out",
              "comments",
              "ScoutedTime"
            ]
          },
          "values": {
            "description": "The list of entries to be added into the database",
            "type": "array",
            "minItems": 1,
            "uniqueItems": true,
            "items": {
              "type": "array",
              "minItems": 37,
              "maxItems": 37,
              "items": {
                "type": "string"
              }
            }
          }
        },
        "required": [
          "header",
          "values"
        ]
      }
    }
  ],
  "required": [
    "title",
    "header",
    "values"
  ]
}