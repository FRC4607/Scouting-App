{
  "heading": "Crescendo - 2024 (Pit Scouting)",
  "logo": "logo.png",
  "skipTeamSelection": true,
  "pages": [
    {
      "name": "Robot",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text"
        },
        {
          "name": "Team Number",
          "type": "number"
        },
        {
          "name": "Length",
          "type": "number"
        },
        {
          "name": "Width",
          "type": "number"
        },
        {
          "name": "Number of Batteries",
          "type": "number"
        },
        {
          "name": "Weight",
          "type": "number"
        },
        {
          "name": "Drivetrain Type",
          "type": "dropdown",
          "options": [
            "Tank Drive",
            "Mecanum",
            "Swerve",
            "Other"
          ]
        },
        {
          "name": "Swerve Motor Type",
          "type": "dropdown",
          "options": [
            "Neo",
            "Falcon",
            "Cim"
          ]
        },
        {
          "name": "Swerve Type",
          "type": "dropdown",
          "options": [
            "No swerve",
            "L1",
            "L2",
            "L3",
            "L4",
            "Neo",
            "Falcon",
            "Custom"
          ]
        }
      ]
    },
    {
      "name": "Gameplay and Programming",
      "widgets": [
        {
          "name": "Cycle Time",
          "type": "number"
        },
        {
          "name": "Can Pick Up",
          "type": "multicheckbox",
          "options": [
            "Ground",
            "Source"
          ]
        },
        {
          "name": "Can Score In",
          "type": "multicheckbox",
          "options": [
            "Speaker",
            "Amp",
            "Trap"
          ]
        }
      ]
    },
    {
      "name": "Processes",
      "widgets": [
        {
          "name": "Number of Batteries",
          "type": "number",
          "min": 1
        },
        {
          "name": "Battery Maintenance",
          "type": "checkbox"
        },
        {
          "name": "Battery Maintenance_text",
          "noLabel": true,
          "type": "text",
          "col": 2
        },
        {
          "name": "Batteries Get Tested",
          "type": "checkbox"
        },
        {
          "name": "Main Battery Brand",
          "type": "dropdown",
          "options": [
            "MK",
            "Interstate",
            "Duracell",
            "Mighty Max",
            "Other (comments)"
          ]
        },
        {
          "name": "Our batteries needed during playoffs",
          "type": "checkbox"
        },
        {
          "name": "Pit Checklist",
          "type": "checkbox"
        },
        {
          "name": "Does Scouting",
          "type": "checkbox"
        },
        {
          "name": "Willing to trade scouting data",
          "type": "checkbox"
        }
      ]
    },
    {
      "name": "Pictures and Comments",
      "widgets": [
        {
          "name": "Note: You can upload multiple pictures/videos for each section!",
          "type": "label",
          "colspan": 2,
          "labelColspan": 2
        },
        {
          "name": "space",
          "type": "spacer"
        },
        {
          "name": "Battery Storage and Charging Area",
          "type": "pictureupload"
        },
        {
          "name": "Battery Location On Robot",
          "type": "pictureupload"
        },
        {
          "name": "Battery Connector Close Up",
          "type": "pictureupload"
        },
        {
          "name": "Main Power Switch Location",
          "type": "pictureupload"
        },
        {
          "name": "Robot Close Ups and Features",
          "type": "pictureupload"
        },
        {
          "name": "space",
          "type": "spacer"
        },
        {
          "name": "Comments",
          "type": "label"
        },
        {
          "name": "Scoring Methods",
          "type": "multicheckbox",
          "options": [
            "Manual",
            "Auto"
          ]
        },
        {
          "name": "Preferred Pickup Location",
          "type": "dropdown",
          "options": [
            "Ground",
            "Source"
          ],
          "defaultOption": true
        },
        {
          "name": "Comments",
          "type": "textarea",
          "colspan": 2
        }
      ]
    }
  ]
}
