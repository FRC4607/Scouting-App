{
  "heading": "Pit Scouting",
  "logo": "Charged_Up_Logo.png",
  "skipTeamSelection": true,
  "pages": [
    {
      "name": "Robot",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text"
        },
        {
          "name": "Team Number",
          "type": "spinbox",
          "allowKeyboardInput": true,
          "min": 1
        },
        {
          "name": "Drivetrain Type",
          "type": "dropdown",
          "options": [
            "Tank Drive",
            "Mecanum",
            "Swerve",
            "Other"
          ]
        },
        {
          "name": "Frame Width",
          "type": "spinbox",
          "allowKeyboardInput": true,
          "min": 20,
          "max": 40
        },
        {
          "name": "Frame Length",
          "type": "spinbox",
          "allowKeyboardInput": true,
          "min": 20,
          "max": 40
        },
        {
          "name": "Weight",
          "type": "spinbox",
          "allowKeyboardInput": true,
          "min": 50,
          "max": 150
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Bumper Height",
          "type": "spinbox",
          "allowKeyboardInput": true,
          "min": 0,
          "max": 20
        },
        {
          "name": "Bumper Depth",
          "type": "spinbox",
          "allowKeyboardInput": true,
          "min": 0,
          "max": 20
        },
        {
          "name": "Bumper Mounting Configuration",
          "type": "text"
        },
        {
          "name": "Using break mode on drivetrain",
          "type": "checkbox"
        },
        {
          "name": "If swerve what kind of modules",
          "type": "dropdown",
          "options": [
            "SDS L1-L4",
            "SDS MK3",
            "SDS MK4",
            "SDS MK4i",
            "REV MAXSwerve",
            "Swerve X",
            "Other COTS",
            "Custom",
            "Other (comments)"
          ]
        },
        {
          "name": "Frequency of replacing wheels",
          "type": "text"
        },
        {
          "name": "Pneumatics in use",
          "type": "checkbox"
        }
      ]
    },
    {
      "name": "Gameplay and Programming",
      "widgets": [
        {
          "name": "Programming Language",
          "type": "dropdown",
          "options": [
            "Java",
            "C++",
            "Python",
            "Labview"
          ]
        },
        {
          "name": "Mobility Capable",
          "type": "checkbox"
        },
        {
          "name": "Preferred Play Style",
          "type": "dropdown",
          "options": [
            "Defense",
            "Offense",
            "Runner"
          ]
        }
      ]
    },
    {
      "name": "Processes",
      "widgets": [
        {
          "name": "Number of Batteries",
          "type": "number",
          "min": 1
        },
        {
          "name": "Battery Maintenance",
          "type": "checkbox"
        },
        {
          "name": "Battery Maintenance_text",
          "noLabel": "true",
          "type": "text",
          "col": 2
        },
        {
          "name": "Batteries Get Tested",
          "type": "checkbox"
        },
        {
          "name": "Main Battery Brand",
          "type": "dropdown",
          "options": [
            "MK",
            "Interstate",
            "Duracell",
            "Mighty Max",
            "Other (comments)"
          ]
        },
        {
          "name": "Our batteries needed during playoffs",
          "type": "checkbox"
        },
        {
          "name": "Pit Checklist",
          "type": "checkbox"
        },
        {
          "name": "Does Scouting",
          "type": "checkbox"
        },
        {
          "name": "Willing to trade scouting data",
          "type": "checkbox"
        }
      ]
    },
    {
      "name": "Pictures and Comments",
      "widgets": [
        {
          "name": "Note: You can upload multiple pictures/videos for each section!",
          "type": "label",
          "colspan": 2,
          "labelColspan": 2
        },
        {
          "name": "space",
          "type": "spacer"
        },
        {
          "name": "Battery Storage and Charging Area",
          "type": "pictureupload"
        },
        {
          "name": "Battery Location On Robot",
          "type": "pictureupload"
        },
        {
          "name": "Battery Connector Close Up",
          "type": "pictureupload"
        },
        {
          "name": "Main Power Switch Location",
          "type": "pictureupload"
        },
        {
          "name": "Robot Close Ups and Features",
          "type": "pictureupload"
        },
        {
          "name": "space",
          "type": "spacer"
        },
        {
          "name": "Comments",
          "type": "label"
        },
        {
          "name": "Comments",
          "type": "textarea",
          "colspan": 2
        }
      ]
    }
  ]
}