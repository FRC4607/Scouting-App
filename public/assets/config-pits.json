{
  "heading": "Reefscape - 2025 (Pit Scouting)",
  "logo": "logo.png",
  "skipTeamSelection": true,
  "pages": [
    {
      "name": "Robot",
      "widgets": [
        {
          "displayName": "Scouter Name",
          "name": "scouter_name",
          "type": "text",
          "validation": {
            "comparison": "greater",
            "value": 0
          }
        },
        {
          "displayName": "Team Number",
          "name": "team_number",
          "type": "number",
          "validation": {
            "comparison": "inRange",
            "value": [
              1,
              65535
            ]
          }
        },
        {
          "displayName": "Programming Language",
          "name": "prog_lang",
          "type": "dropdown",
          "options": [
            "Java",
            "C++",
            "Python",
            "Labview"
          ]
        },
        {
          "displayName": "Battery Brand",
          "name": "bat_brand",
          "type": "dropdown",
          "options": [
            "MK",
            "Duracell",
            "Interstate",
            "Mighty Max",
            "Power Sonic",
            "Other"
          ]
        },
        {
          "displayName": "Other Brand",
          "name": "otherbrand",
          "type": "textarea"
        },
        {
          "displayName": "Amount of Batteries",
          "name": "num_bat",
          "type": "number",
          "validation": {
            "comparison": "greaterOrEqual",
            "value": 1
          }
        },
        {
          "displayName": "Battery Condition",
          "name": "bat_condition",
          "type": "textarea"
        },
        {
          "displayName": "Spare Parts",
          "name": "spareparts",
          "type": "checkbox"
        },
        {
          "displayName": "Component Protection",
          "name": "component_prot",
          "type": "checkbox"
        }
        
      ]
    }
  ]
}