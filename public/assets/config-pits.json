{
  "heading": "Pit Scouting",
  "logo": "CHARGED_UP_Logo_Horiz_RGB_FullColorReverse.png",
  "skipTeamSelection": true,
  "pages": [
    {
      "name": "Robot",
      "widgets": [
        {
          "name": "Team Number",
          "type": "number"
        },
        {
          "name": "Scouter Name",
          "type": "text"
        },
        {
          "name": "Drivetrain",
          "type": "dropdown",
          "options": [
            "Tank Drive",
            "Mecanum",
            "Swerve",
            "Other"
          ]
        },
        {
          "name": "Frame Width",
          "type": "number",
          "min": 20,
          "max": 40
        },
        {
          "name": "Frame Length",
          "type": "number",
          "min": 20,
          "max": 40
        },
        {
          "name": "Weight",
          "type": "number",
          "min": 50,
          "max": 150
        }
      ]
    },
    {
      "name": "Gameplay and Programing",
      "widgets": [
        {
          "name": "Programing Language",
          "type": "dropdown",
          "options": [
            "Java",
            "C++",
            "Python",
            "Labview"
          ]
        },
        {
          "name": "Mobility Capable",
          "type": "checkbox"
        },
        {
          "name": "Preferred Play Style",
          "type": "dropdown",
          "options": [
            "Defense",
            "Offense",
            "Runner"
          ]
        }
      ]
    },
    {
      "name": "Processes",
      "widgets": [
        {
          "name": "Number of Batteries",
          "type": "number"
        },
        {
          "name": "Battery Maintenance",
          "type": "checkbox"
        },
        {
          "name": "Battery Maintenance-text",
          "noLabel": "true",
          "type": "text",
          "col": 2
        },
        {
          "name": "Pit Checklist",
          "type": "checkbox"
        },
        {
          "name": "Scouting",
          "type": "checkbox"
        }
      ]
    },
    {
      "name": "Comments and Pictures",
      "widgets": [
        {
          "name": "Battery Area",
          "type": "pictureUpload"
        },
        {
          "name": "Robot close up",
          "type": "pictureUpload"
        },
        {
          "name": "Comments",
          "type": "label"
        },
        {
          "name": "Comments",
          "noLabel": true,
          "type": "textarea"
        }
      ]
    }
  ]
}