{
  "heading": "Crescendo - 2024 (Pit Scouting)",
  "logo": "logo.png",
  "skipTeamSelection": true,
  "pages": [
    {
      "name": "Robot",
      "widgets": [
        {
          "displayName": "Scouter Name",
          "name": "scouter_name",
          "type": "text"
        },
        {
          "displayName": "Team Number",
          "name": "team_number",
          "type": "number"
        },
        {
          "displayName": "Length",
          "name": "length",
          "type": "number"
        },
        {
          "displayName": "Width",
          "name": "width",
          "type": "number"
        },
        {
          "displayName": "Weight",
          "name": "weight",
          "type": "number"
        },
        {
          "displayName": "Drivetrain Type",
          "name": "dt_type",
          "type": "dropdown",
          "options": [
            "Tank Drive",
            "Mecanum",
            "Swerve",
            "Other"
          ]
        },
        {
          "displayName": "Swerve Motor Type",
          "name": "swerve_motor",
          "type": "dropdown",
          "options": [
            "Neo",
            "Falcon",
            "Cim"
          ]
        },
        {
          "displayName": "Swerve Type",
          "name": "swerve_type",
          "type": "dropdown",
          "options": [
            "No swerve",
            "L1",
            "L2",
            "L3",
            "L4",
            "Neo",
            "Falcon",
            "Custom"
          ]
        }
      ]
    },
    {
      "name": "Gameplay and Programming",
      "widgets": [
        {
          "displayName": "Cycle Time",
          "name": "cycle_time",
          "type": "number"
        },
        {
          "displayName": "Can Pick Up",
          "name": "can_pickup",
          "type": "multicheckbox",
          "options": [
            "Ground",
            "Source"
          ]
        },
        {
          "displayName": "Can Score In",
          "name": "scoring_locations",
          "type": "multicheckbox",
          "options": [
            "Speaker",
            "Amp",
            "Trap"
          ]
        }
      ]
    },
    {
      "name": "Processes",
      "widgets": [
        {
          "displayName": "Number of Batteries",
          "name": "num_batt",
          "type": "number",
          "min": 1
        },
        {
          "displayName": "Battery Maintenance",
          "name": "does_batt_maint",
          "type": "checkbox"
        },
        {
          "displayName": "Battery Maintenance_text",
          "name": "batt_maint_txt",
          "noLabel": true,
          "type": "text",
          "col": 2
        },
        {
          "displayName": "Batteries Get Tested",
          "name": "batt_testing",
          "type": "checkbox"
        },
        {
          "displayName": "Main Battery Brand",
          "name": "batt_brand",
          "type": "dropdown",
          "options": [
            "MK",
            "Interstate",
            "Duracell",
            "Mighty Max",
            "Other (comments)"
          ]
        },
        {
          "displayName": "Our batteries needed during playoffs",
          "name": "batt_needed",
          "type": "checkbox"
        },
        {
          "displayName": "Pit Checklist",
          "name": "pit_checklist",
          "type": "checkbox"
        },
        {
          "displayName": "Does Scouting",
          "name": "does_scouting",
          "type": "checkbox"
        },
        {
          "displayName": "Willing to trade scouting data",
          "name": "trade_scouting_data",
          "type": "checkbox"
        }
      ]
    },
    {
      "name": "Pictures and Comments",
      "widgets": [
        {
          "name": "Note: You can upload multiple pictures/videos for each section!",
          "type": "label",
          "colspan": 2,
          "labelColspan": 2
        },
        {
          "displayName": "space",
          "type": "spacer"
        },
        {
          "displayName": "Battery Storage and Charging Area",
          "name": "batt_storchrg",
          "type": "pictureupload"
        },
        {
          "displayName": "Battery Location On Robot",
          "name": "batt_loc",
          "type": "pictureupload"
        },
        {
          "displayName": "Battery Connector Close Up",
          "name": "batt_con",
          "type": "pictureupload"
        },
        {
          "displayName": "Main Power Switch Location",
          "name": "powersw_loc",
          "type": "pictureupload"
        },
        {
          "displayName": "Robot Close Ups and Features",
          "name": "misc_pics",
          "type": "pictureupload"
        },
        {
          "displayName": "space",
          "type": "spacer"
        },
        {
          "displayName": "Scoring Methods",
          "name": "scoring_method",
          "type": "multicheckbox",
          "options": [
            "Manual",
            "Auto"
          ]
        },
        {
          "displayName": "Preferred Pickup Location",
          "name": "preferred_pickup",
          "type": "dropdown",
          "options": [
            "Ground",
            "Source"
          ],
          "defaultOption": true
        },
        {
          "displayName": "Comments",
          "name": "comments",
          "type": "textarea",
          "colspan": 2
        }
      ]
    }
  ]
}
