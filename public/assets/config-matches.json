{
  "heading": "Crescendo - 2024",
  "logo": "logo.png",
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "displayName": "Scouter Name",
          "name": "scouter_name",
          "type": "text",
          "validation": {
            "comparison": "greater",
            "value": 0
          }
        },
        {
          "displayName": "Robot Starting Position",
          "name": "starting_pos",
          "type": "dropdownwithpictures",
          "options": [
            "Subwoofer: Amp",
            "Subwoofer: Speaker",
            "Subwoofer: Podium",
            "Amp Note",
            "Speaker Note",
            "Podium Note",
            "Between Amp and Speaker Notes",
            "Between Speaker and Podium Notes",
            "Note Closest to Source",
            "Note 2nd Closest to Source",
            "Other"
          ],
          "files": [
            "start_loc/sw_amp.png",
            "start_loc/sw_speaker.png",
            "start_loc/sw_podium.png",
            "start_loc/note_amp.png",
            "start_loc/note_speaker.png",
            "start_loc/note_podium.png",
            "start_loc/note_mid_amp.png",
            "start_loc/note_mid_podium.png",
            "start_loc/note_source_close.png",
            "start_loc/note_source_far.png"
          ]
        }
      ]
    },
    {
      "name": "Autonomous",
      "widgets": [
        {
          "displayName": "Pre Load Scoring",
          "name": "pre_load_score",
          "type": "checkbox"
        },
        {
          "displayName": "Left Starting Zone",
          "name": "mobility",
          "type": "checkbox"
        },
        {
          "displayName": "Amp Notes",
          "name": "auto_amp",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "type": "picture",
          "file": "2160xDarkCroppedFixed.png",
          "rowspan": 2,
          "colspan": 2,
          "col": 1,
          "width": 600
        },
        {
          "displayName": "Zone 1 Shots Made",
          "name": "zone1_shot_made_auto",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 1 Shots Miss",
          "name": "zone1_shot_miss_auto",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 2 Shots Made",
          "name": "zone2_shot_made_auto",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 2 Shots Miss",
          "name": "zone2_shot_miss_auto",
          "type": "spinbox",
          "min": 0,
          "max": 255
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [
        {
          "displayName": "Can Go Under Stage",
          "name": "under_stage",
          "type": "checkbox"
        },
        {
          "displayName": "Defense",
          "name": "defense",
          "type": "checkbox"
        },
        {
          "displayName": "Pickup Method",
          "name": "pickup_method",
          "type": "multicheckbox",
          "options": [
            "Ground",
            "Source"
          ]
        },
        {
          "type": "picture",
          "file": "2160xDarkCroppedFixed.png",
          "rowspan": 2,
          "colspan": 2,
          "col": 1,
          "width": 600
        },
        {
          "displayName": "Zone 1 Shots Made",
          "name": "zone1_shot_made",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 1 Shots Miss",
          "name": "zone1_shot_miss",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 2 Shots Made",
          "name": "zone2_shot_made",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 2 Shots Miss",
          "name": "zone2_shot_miss",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 3 Shots Made",
          "name": "zone3_shot_made",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 3 Shots Miss",
          "name": "zone3_shot_miss",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 4 Shots Made",
          "name": "zone4_shot_made",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Zone 4 Shots Miss",
          "name": "zone4_shot_miss",
          "type": "spinbox",
          "min": 0,
          "max": 255
        },
        {
          "displayName": "Amp Notes",
          "name": "teleop_amp",
